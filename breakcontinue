i = 0
while True:
    print(">{}번째 반복중입니다".format(i))
    i += 1

    user_input = input ("프로그램을 종료하시겠습니까?(Y)")
    if user_input in ("Y", "y"):
        print("프로그램이 종료되었습니다")
        break


numbers = [7 ,17, 33, 44, 45, 9]

print()



for number in numbers:
    if number > 10:
        continue
    print(number)

print()






for i in range(10):
    if i % 2 == 0:
        continue
    print(i)

print()

i = 0
while i < 10:
    i += 1
    if i % 2 == 0:
        continue
    print(i)
