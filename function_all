print()
print()

def add(a,b):
    return a + b
print(add(5,9))


def add(a,b):
    return a + b
x = add(5,9)
print(x)
print()
print()

def f_1(x):
    return (2 * x) + 1
print(f_1(10))


def f_2(x):
    return (x ** 2) + (2 * x) +1
print(f_2(10))
print()
print()


def mul(*value):
    변수 = 1
    for i in value:
        변수 *= i
    return 변수

print(mul(5,7,9,10))

