
def function(일반매개,*가변매개,기본매개=10):
    print(일반매개)
    print(가변매개)
    print(기본매개)
function(0,1,2,3,4,5,6,7,기본매개=55)
print()
print()
print("타쿠대디","시온대디","대디유댕",sep="||")
