
math = __import__ ("math")
print(math.pi)
print(math.sin(10))
print()

import math
print(math.pi)
print(math.sin(10))

import math as 수학
print(수학.pi)
print(수학.sin(10))


from math import pi, sin
print(pi)
print(sin(10))

from math import *
print(pi)
print(sin(10))


print()
math = __import__ ("math")
import math
import math as 수학
from math import pi, sin
from math import *
